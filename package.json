{
  "name": "n8n-nodes-aptos",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Aptos blockchain providing 22 resources and 200+ operations for account management, transactions, NFTs, staking, governance, and Move smart contracts. Includes trigger node for real-time event monitoring.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-aptos.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-aptos#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-aptos/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "aptos",
    "blockchain",
    "move",
    "web3",
    "cryptocurrency",
    "nft",
    "digital-assets",
    "defi",
    "staking",
    "governance",
    "smart-contracts",
    "apt",
    "automation"
  ],
  "main": "dist/nodes/Aptos/Aptos.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AptosNetwork.credentials.js",
      "dist/credentials/AptosIndexer.credentials.js",
      "dist/credentials/AptosFaucet.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Aptos/Aptos.node.js",
      "dist/nodes/Aptos/AptosTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:coverage": "jest --coverage",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.26.0",
    "axios": "^1.6.7",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
